<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        style="@style/TitleStyle"
        android:text="设置中心"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <event.study.liuqi.mobilesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/event.study.liuqi.mobilesafe"
        android:id="@+id/update_set"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="自动更新设置"
        mobilesafe:desoff="自动更新已关闭"
        mobilesafe:deson="自动更新已开启"
        >
    </event.study.liuqi.mobilesafe.view.SettingItemView>
    <event.study.liuqi.mobilesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/event.study.liuqi.mobilesafe"
        android:id="@+id/show_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="电话归属地显示设置"
        mobilesafe:desoff="归属地显示已关闭"
        mobilesafe:deson="归属地显示已开启"
        >
    </event.study.liuqi.mobilesafe.view.SettingItemView>
    <event.study.liuqi.mobilesafe.view.SettingClickView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/event.study.liuqi.mobilesafe"
        android:id="@+id/address_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:itemtitle="设置归属地显示风格"
        mobilesafe:itemparm="透明"
        >
    </event.study.liuqi.mobilesafe.view.SettingClickView>
    <event.study.liuqi.mobilesafe.view.SettingClickView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/event.study.liuqi.mobilesafe"
        android:id="@+id/address_place"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:itemtitle="设置归属地框位置"
        mobilesafe:itemparm="设置归属地框提示位置"
        >
    </event.study.liuqi.mobilesafe.view.SettingClickView>

    <event.study.liuqi.mobilesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/event.study.liuqi.mobilesafe"
        android:id="@+id/black_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="黑名单拦截设置"
        mobilesafe:desoff="黑名单拦截已关闭"
        mobilesafe:deson="黑名单拦截已开启"
        >
    </event.study.liuqi.mobilesafe.view.SettingItemView>

</LinearLayout>